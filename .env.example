# ===========================================
# DOCKER / POSTGRESQL
# Utilisé par docker-compose.yml
# ===========================================
DB_PASSWORD=

# JWT pour PostgREST (min 64 caractères)
# Générer avec: openssl rand -base64 48
PGRST_JWT_SECRET=

# ===========================================
# BASE DE DONNÉES (Application)
# ===========================================
DATABASE_URL=postgresql://webstudio:${DB_PASSWORD}@webstudio-postgres:5432/webstudio
DIRECT_URL=postgresql://webstudio:${DB_PASSWORD}@webstudio-postgres:5432/webstudio
